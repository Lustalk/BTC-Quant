[flake8]
max-line-length = 127
max-complexity = 10
exclude = 
    .git,
    __pycache__,
    .pytest_cache,
    .venv,
    venv,
    env,
    .env,
    build,
    dist,
    *.egg-info
ignore = 
    E203,
    E501,
    W503,
    F401,
    F403,
    F405
per-file-ignores =
    __init__.py:F401
    tests/*:F401 